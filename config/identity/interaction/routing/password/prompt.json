{
  "@context": [
    "https://linkedsoftwaredependencies.org/bundles/npm/@solid/community-server/^7.0.0/components/context.jsonld",
   "https://linkedsoftwaredependencies.org/bundles/npm/simple-register-module/^7.0.0/components/context.jsonld"
  ],
  "@graph": [
    {
      "@id": "urn:solid-server:default:PromptHandler",
      "@type": "PromptHandler",
      "promptRoutes": [
        {
          "PromptHandler:_promptRoutes_key": "create",
          "PromptHandler:_promptRoutes_value": { "@id": "urn:my-simple-register:default:CreateLoginPasswordRoute" }
        }
      ]
    },
    {
      "comment": "Ensure create prompt is first",
      "@type": "Override",
      "overrideInstance": { "@id": "urn:solid-server:default:PromptFactory" },
      "overrideParameters": {
        "@type": "SequenceHandler",
        "handlers": [
          {
            "@type": "AccountPromptFactory",
            "webIdStore": { "@id": "urn:solid-server:default:WebIdStore" },
            "cookieStore": { "@id": "urn:solid-server:default:CookieStore" },
            "cookieName": { "@id": "urn:solid-server:default:value:accountCookieName" }
          },
          {
            "@type": "CreatePromptFactory"
          }
        ]
      }
    }
  ]
}

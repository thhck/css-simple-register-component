{
  "name": "simple-register-module",
  "version": "7.0.0",
  "description": "A minimal example of a repository containing an injectable Community Solid Server module",
  "repository": "git@github.com:CommunitySolidServer/simple-register-module.git",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "lsd:module": "https://linkedsoftwaredependencies.org/bundles/npm/simple-register-module",
  "lsd:components": "dist/components/components.jsonld",
  "lsd:contexts": {
    "https://linkedsoftwaredependencies.org/bundles/npm/simple-register-module/^7.0.0/components/context.jsonld": "dist/components/context.jsonld"
  },
  "lsd:importPaths": {
    "https://linkedsoftwaredependencies.org/bundles/npm/simple-register-module/^7.0.0/components/": "dist/components/",
    "https://linkedsoftwaredependencies.org/bundles/npm/simple-register-module/^7.0.0/config/": "config/",
    "https://linkedsoftwaredependencies.org/bundles/npm/simple-register-module/^7.0.0/dist/": "dist/"
  },
  "scripts": {
    "start": "community-solid-server -c @css:config/file.json simple-register-partial.json -f .data -m .",
    "start-alt": "community-solid-server -c simple-register-file.json -f .data -m .",
    "build": "npm run build:ts && npm run build:components",
    "build:components": "componentsjs-generator -s src -c dist/components -i .componentsignore -r simple-register",
    "build:ts": "tsc",
    "prepare": "npm run build",
    "test": "jest"
  },
  "files": [
    "dist",
    "config"
  ],
  "dependencies": {
    "@solid/community-server": "^7.0.3"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/jest": "^29.5.12",
    "componentsjs-generator": "^3.1.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "license": "MIT"
}
